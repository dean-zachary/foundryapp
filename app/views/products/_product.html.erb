 
    <div class= "container-fluid col-sm-6 col-md-6" style="padding-left: 0px; padding-right: 0px;">
      <% cache product do %>
      <tr>
        <td><%= product.name %></td>
        <td><%= product.description %></td>
        <td><%= link_to image_tag(product.image_url, class: "img-responsive"), product %></td>
        <td><%= product.color %></td>
        <td><%= product.price %></td>
        <!--<td><%= link_to 'Show', product %></td>
        <td><%= link_to 'Edit', edit_product_path(product) %></td>
        <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
          <% end %>
  </div>
